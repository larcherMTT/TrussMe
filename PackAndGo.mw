                           { data::{list(`=`), set(`=`)} := [], 
                             scaling::numeric := 1 }, $ ) :: function

    # Plot the BEAM object &lt;obj&gt; given a list or set of substitution &lt;data&gt;.
    PlotBeam( obj::BEAM, 
